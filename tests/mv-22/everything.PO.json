{
  "?predefs": {
    "#": "predefined models",
    "?DATE": "$DATE",
    "?DATETIME": "$DATETIME",
    "?TIME": "$TIME",
    "?JSON": "$JSON",
    "?REGEX": "$REGEX",
    "?EXREG": "$EXREG",
    "?ANY": "$ANY",
    "?NONE": "$NONE",
    "?UUID": "$UUID",
    "?URL": "$URL",
    "?URI": "$URI",
    "?EMAIL": "$EMAIL",
    "?NULL": "$NULL",
    "?BOOL": "$BOOL",
    "?BOOLEAN": "$BOOLEAN",
    "?INT": "$INT",
    "?INTEGER": "$INTEGER",
    "?I32": "$I32",
    "?I64": "$I64",
    "?U32": "$U32",
    "?U64": "$U64",
    "?FLOAT": "$FLOAT",
    "?F32": "$F32",
    "?F64": "$F64",
    "?NUMBER": "$NUMBER",
    "?STRING": "$STRING"
  },
  "?null": {
    "#": "3 representations of null",
    "?n0": null,
    "?n1": "$NULL",
    "?n2": "=null"
  },
  "?bool": {
    "#": "booleans: inference, predefs, constants",
    "?b0": true,
    "?b1": "$BOOL",
    "?b2": "$BOOLEAN",
    "?b3": "=true",
    "?b4": "=false"
  },
  "?int": {
    "#": "integers: inference, predefs, constants",
    "?i0": 0,
    "?i1": 1,
    "?i2": -1,
    "?i3": "$INT",
    "?i4": "$INTEGER",
    "?i5": "$I32",
    "?i6": "$I64",
    "?i7": "$U32",
    "?i8": "$U64",
    "?i9": "=42",
    "?ia": "=-42"
  },
  "?float": {
    "#": "floats: inference, predefs, constants",
    "?f0": 0.0,
    "?f1": 1.0,
    "?f2": -1.0,
    "?f3": "$FLOAT",
    "?f4": "$F32",
    "?f5": "$F64",
    "?f6": "=3.1415927",
    "?f7": "=1.0E100",
    "?f8": "=-42.1"
  },
  "?string": {
    "#": "strings: inference, predef, constants, regex",
    "?s0": "",
    "?s1": "$STRING",
    "?s2": "Susie",
    "?s3": "_Calvin",
    "?s4": "/^(Calvin|Susie)$/"
  },
  "?array": {
    "#": "example arrays",
    "?a0": [
      0
    ],
    "?a1": [
      ""
    ],
    "?a2": [
      "$DATE"
    ],
    "?a3": [
      true
    ]
  },
  "?tuple": {
    "#": "tuple items have a type",
    "?t0": [],
    "?t1": {
      "@": [
        0
      ],
      "=": 1
    },
    "?t2": [
      0,
      0
    ],
    "?t3": [
      true,
      0,
      ""
    ],
    "?t4": {
      "@": [
        "",
        0
      ],
      ">=": 2
    }
  },
  "?object": {
    "#": "object map property names to values",
    "?e0": {},
    "?e1": {
      "a": 0,
      "_b": 0,
      "!c": 0
    },
    "?e2": {
      "?a": 0
    },
    "?e3": {
      "/^a/": 0
    },
    "?e4": {
      "$DATE": 0
    },
    "?e5": {
      "": "0"
    },
    "?e6": {
      "": "$ANY"
    }
  },
  "?enum": {
    "#": "an enumeration only contains constants",
    "?e0": {
      "|": [
        "=true",
        "=false"
      ]
    },
    "?e1": {
      "|": [
        "=200",
        "=201",
        "=204"
      ]
    },
    "?e2": {
      "|": [
        "_Susie",
        "_Calvin",
        "_Hobbes"
      ]
    }
  },
  "?or": {
    "#": "soft alternative, first match",
    "?o0": {
      "|": [
        true,
        -1
      ]
    },
    "?o1": {
      "|": [
        "$DATE",
        "$TIME"
      ]
    },
    "?o2": {
      "|": [
        0,
        "$UUID",
        [
          "$ANY"
        ],
        {
          "": "$ANY"
        }
      ]
    }
  },
  "?xor": {
    "#": "hard alternative, only one must match",
    "?x0": {
      "|": [
        true,
        -1
      ]
    },
    "?x1": {
      "^": [
        "$ANY",
        0
      ]
    }
  },
  "?and": {
    "#": "conjunction, all must match",
    "?a0": {
      "&": [
        "$DATE",
        "/^2020-/"
      ]
    }
  },
  "?merge": {
    "#": "merge object properties",
    "?m0": {
      "a": 0,
      "?b": 0
    },
    "?m1": {
      "|": [
        {
          "a": 0,
          "b": 1
        },
        {
          "a": 0,
          "c": 1
        }
      ]
    },
    "?m2": {
      "|": [
        {
          "a": 0,
          "b": 1
        },
        {
          "a": 0,
          "c": 1
        }
      ]
    },
    "?m3": {
      "|": [
        {
          "a": 0,
          "c": 1
        },
        {
          "a": 0,
          "d": 1
        },
        {
          "b": 0,
          "c": 1
        },
        {
          "b": 0,
          "d": 1
        }
      ]
    }
  },
  "?constraints": {
    "?cii0": "=10",
    "?cii1": {
      "@": 0,
      "!=": 10,
      ">=": 5,
      "<=": 15
    },
    "?cii2": {
      "@": 0,
      "<=": 14,
      ">=": 6
    },
    "?cin0": {
      "@": 0.0,
      "=": 10
    },
    "?cin1": {
      "@": 0.0,
      "!=": 10,
      ">=": 5,
      "<=": 15
    },
    "?cin2": {
      "@": 0.0,
      ">": 5,
      "<": 15
    },
    "?cis0": {
      "@": "",
      "=": 10
    },
    "?cis1": {
      "@": "",
      "!=": 10,
      ">=": 5,
      "<=": 15
    },
    "?cis2": {
      "@": "",
      "<=": 14,
      ">=": 6
    },
    "?cia0": {
      "@": [
        0
      ],
      "=": 10
    },
    "?cia1": {
      "@": [
        0
      ],
      "!=": 10,
      ">=": 5,
      "<=": 15
    },
    "?cia2": {
      "@": [
        0
      ],
      "<=": 14,
      ">=": 6
    },
    "?cio0": {
      "@": {
        "": 0
      },
      "=": 10
    },
    "?cio1": {
      "@": {
        "": 0
      },
      "!=": 10,
      ">=": 5,
      "<=": 15
    },
    "?cio2": {
      "@": {
        "": 0
      },
      "<=": 14,
      ">=": 6
    },
    "?cni0": {
      "@": 0,
      "=": 42.0
    },
    "?cni1": {
      "@": 0,
      "!=": 42.0
    },
    "?cni2": {
      "@": 0,
      ">": 42.0,
      "<": 43.0
    },
    "?cnn0": {
      "@": 0.0,
      "=": 42.1
    },
    "?cnn1": {
      "@": 0.0,
      "!=": 42.1,
      "<=": 99.9,
      ">=": 0.1
    },
    "?cnn2": {
      "@": 0.0,
      ">": 42.0,
      "<": 43.0
    },
    "?css0": {
      "@": "",
      "=": "Susie"
    },
    "?css1": {
      "@": "",
      "!=": "Hobbes"
    },
    "?css2": {
      "@": "",
      ">": "A",
      "<": "Z"
    },
    "?cua0": {
      "@": [
        0
      ],
      "!": true
    }
  },
  "#": "JsonModel 1",
  "$": {
    "a": "_A",
    "b": "_A",
    "#": "Symbol 1",
    "": "./everything"
  },
  "~": "https://json-model.org/models/json-model"
}
