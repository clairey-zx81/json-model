{
  "$comment": "JSON Schema generated by JSON Model Compiler",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Model for the JSON Schema Test Suite tests",
  "$defs": {
    "Version": {
      "type": "string",
      "pattern": "^[0-9a-zA-Z]+(\\.[0-9a-zA-Z]+)*$"
    },
    "Test": {
      "description": "A single test",
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "comment": {
          "type": "string"
        },
        "data": true,
        "valid": {
          "type": "boolean"
        }
      },
      "required": [
        "description",
        "data",
        "valid"
      ],
      "additionalProperties": false
    },
    "Specification": {
      "type": "object",
      "patternProperties": {
        "^rfc\\d+$": {
          "$ref": "#/$defs/Version"
        },
        "^iso\\d+$": {
          "$ref": "#/$defs/Version"
        }
      },
      "properties": {
        "core": {
          "$ref": "#/$defs/Version"
        },
        "validation": {
          "$ref": "#/$defs/Version"
        },
        "ecma262": {
          "$ref": "#/$defs/Version"
        },
        "perl5": {
          "type": "string"
        },
        "quote": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "minProperties": 1
    },
    "TestCase": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "comment": {
          "type": "string"
        },
        "schema": true,
        "tests": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Test"
          },
          "minItems": 1
        },
        "specification": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Specification"
          },
          "minItems": 1,
          "uniqueItems": true
        }
      },
      "required": [
        "description",
        "schema",
        "tests"
      ],
      "additionalProperties": false
    }
  },
  "type": "array",
  "items": {
    "$ref": "#/$defs/TestCase"
  },
  "minItems": 1
}
