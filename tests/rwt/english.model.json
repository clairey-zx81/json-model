{
  "#": "JSON Model with English Keywords",
  "~": "https://json-model.org/models/l10n",
  "$": { "Model": "$https://json-model.org/models/json-model-v2" },
  "%": {
    ".model": "@",
    ".defs": "$",
    ".trafo": "%",
    ".or": "|",
    ".and": "&",
    ".xor": "^",
    ".merge": "+",
    ".unique": "!",
    ".del": "/",
    ".add": "*"
  },
  "@": "$Model"
}
