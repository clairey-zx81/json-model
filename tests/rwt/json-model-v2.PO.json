{
  "#": "JsonModel 0",
  "$": {
    "": "https://json-model.org/models/json-model-v2",
    "#": "Definitions, without predefs of any kind",
    "Add": {
      "_+": [
        "$Model"
      ]
    },
    "And": {
      "_&": [
        "$Model"
      ]
    },
    "Any": {
      "|": [
        "$Val",
        [
          "$Any"
        ],
        {
          "": "$Any"
        }
      ]
    },
    "Array": [
      "$Model"
    ],
    "Comment": {
      "?#": ""
    },
    "Constraint": {
      "/^(<=|>=|<|>|\u2265|\u2264)$/": {
        "|": [
          -1,
          -1.0,
          ""
        ]
      },
      "/^(=|!=|\u2260)$/": "$Val",
      "?!": true,
      "_@": "$Model"
    },
    "Defs": {
      "/.+/": "$Model"
    },
    "Elem": {
      "+": [
        "$Comment",
        {
          "|": [
            "$Constraint",
            "$Or",
            "$And",
            "$Xor",
            "$Add",
            "$Obj"
          ]
        }
      ]
    },
    "Model": {
      "|": [
        "$Val",
        "$Array",
        "$Elem"
      ]
    },
    "None": {
      "|": []
    },
    "Obj": {
      "": "$Model",
      "/^[@|&^+/*]$/": "$None"
    },
    "Or": {
      "_|": [
        "$Model"
      ]
    },
    "Rename": {
      "/^\\..*$/": "/^([|&^+/*@~<>=!]|<=|>=|!=)$/"
    },
    "Rewrite": {
      "/^\\$.*$/": "$Trafo"
    },
    "Root": {
      "+": [
        {
          "#": "Special keywords only at the root of a model",
          "?$": {
            "+": [
              "$Comment",
              {
                "?": "$Url"
              },
              "$Defs"
            ]
          },
          "?%": {
            "+": [
              "$Comment",
              "$Rename",
              "$Rewrite"
            ]
          },
          "?~": "$Url"
        },
        "$Elem"
      ]
    },
    "RootModel": {
      "|": [
        "$Val",
        "$Array",
        "$Root"
      ]
    },
    "Trafo": {
      "|": [
        "$Model",
        {
          "+": [
            "$Comment",
            {
              "|": [
                {
                  "?*": {
                    "|": [
                      [
                        "$ANY"
                      ],
                      {
                        "": "$Model"
                      }
                    ]
                  },
                  "_/": [
                    "$Any"
                  ]
                },
                {
                  "?/": [
                    "$Any"
                  ],
                  "_*": {
                    "|": [
                      [
                        "$ANY"
                      ],
                      {
                        "": "$Model"
                      }
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    "Url": "/^((file|https?)://.+|\\./.*|\\.\\./.*)$/",
    "Val": {
      "|": [
        null,
        true,
        -1,
        -1.0,
        ""
      ]
    },
    "Xor": {
      "_^": [
        "$Model"
      ]
    }
  },
  "@": "$RootModel",
  "~": "https://json-model.org/models/json-model-v2"
}
