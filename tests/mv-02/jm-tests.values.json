[
  "simple 2-tuple expectation/value tests",
  [ true, [] ],
  [ true, [[ true, "$ANY" ]] ],
  [ true, [[ true, "$ANY" ], [ false, "$NONE" ]] ],
  [ false, [ 1 ] ],
  [ false, [ { "hello": "", "?world": "" } ] ],
  [ false, null ],
  [ false, 1 ],
  [ false, "Hello World!" ],
  [ false, {} ],

  "same with 3-tuple expectation/name/value tests",
  [ true, "", [] ],
  [ true, "", [[ true, "$ANY" ]] ],
  [ true, "", [[ true, "$ANY" ], [ false, "$NONE" ]] ],
  [ false, "", [ 1 ] ],
  [ false, "", [ { "hello": "", "?world": "" } ] ],
  [ false, "", null ],
  [ false, "", 1 ],
  [ false, "", "Hello World!" ],
  [ false, "", {} ]
]
