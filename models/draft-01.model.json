{
  "#": "JSON Model for JSON Schema Draft 01",
  "$": "Schema",
  "%": {
    "posInt": {"@": 0, ">=": 0},
    "allTypes": {"|": ["null", "boolean", "integer", "number", "string", "array", "object", "any"]}
  },
  "?type": {"|": ["$allTypes", [{"|": ["", "$Schema"]}]]},
  "?properties": { "": "$Schema" },
  "?items": {"|": ["$Schema", ["$Schema"]]},
  "?optional": true,
  "?additionalProperties": {"|": [true, "$Schema"]},
  "?requires": {"|": ["", "$Schema"]},
  "?minimum": 0.0,
  "?maximum": 0.0,
  "?minimumCanEqual": true,
  "?maximumCanEqual": true,
  "?minItems": "$posInt",
  "?maxItems": "$posInt",
  "?pattern": "$REGEX",
  "?minLength": "$posInt",
  "?maxLength": "$posInt",
  "?enum": {"@": ["$ANY"], ">=": 1},
  "?title": "",
  "?description": "",
  "?format": "",
  "?contentEncoding": "",
  "?default": "$ANY",
  "?maxDecimal": "$postInt",
  "?disallow": {"|": ["", [""]]},
  "?extends": {"|": ["$Schema", ["$Schema"]]},
  "?id": "",
  "?$schema": ""
}
