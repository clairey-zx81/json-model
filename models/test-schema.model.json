{
  "#": "Model for the JSON Schema Test Suite tests",
  "~": "https://json-model.org/models/json-model",
  "$": {
    "": "https://json-model.org/models/test-schema",
    "Version": "/^[0-9a-zA-Z]+(\\.[0-9a-zA-Z]+)*$/",
    "Test": {
      "#": "A single test",
      "description": "",
      "?comment": "",
      "data": "$ANY",
      "valid": true
    },
    "Specification": {
      "@": {
        "?core": "$Version",
        "?validation": "$Version",
        "?ecma262": "$Version",
        "?perl5": "",
        "?quote": "",
        "/^rfc\\d+$/": "$Version",
        "/^iso\\d+$/": "$Version"
      },
      ">": 0
    },
    "TestCase": {
      "description": "",
      "?comment": "",
      "schema": "$ANY",
      "tests": {
        "@": [ "$Test" ],
        ">": 0
      },
      "?specification": {
        "@": [ "$Specification" ],
        ">": 0,
        "!": true
      }
    }
  },
  "@": [ "$TestCase" ],
  ">": 0
}
