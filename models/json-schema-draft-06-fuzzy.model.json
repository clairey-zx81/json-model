{
  "#": "JSON Model for JSON Schema Draft 06 (fuzzy version)",
  "~": "https://json-model.org/models/json-model",
  "$": {
    "": "https://json-model.org/models/json-schema-draft-06-fuzzy",
    "schema": "$https://json-model.org/models/json-schema-draft-06"
  },
  "%": {
    "$schema#ObjectSchema": {
      "/": ["?pattern", "?patternProperties"],
      "*": {"": "$ANY", "?pattern": "", "?patternProperties": {"": "$Schema"}}
    }
  },
  "@": "$schema"
}
