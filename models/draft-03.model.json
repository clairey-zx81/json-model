{
  "#": "JSON Model for JSON Schema Draft 03",
  "$": "Schema",
  "%": {
    "posInt": {"@": 0, ">=": 0},
    "strictPosNum": {"@": 0.0, ">": 0.0},
    "allTypes": {"|": ["null", "boolean", "integer", "number", "string", "array", "object", "any"]},
    "distinctSchemaArray": {"@": [{"|": ["", "$Schema"]}], "!": true}
  },
  "?type": {"|": ["$allTypes", [{"|": ["", "$Schema"]}]]},
  "?properties": { "": "$Schema" },
  "?patternProperties": {"": "$Schema"},
  "?additionalProperties": {"|": [true, "$Schema"]},
  "?items": {"|": ["$Schema", ["$Schema"]]},
  "?additionalItems": {"|": [true, "$Schema"]},
  "?dependencies": {"": {"|": ["", [""], "$Schema"]}},
  "?required": true,
  "?minimum": 0.0,
  "?maximum": 0.0,
  "?exclusiveMaximum": true,
  "?exclusiveMinimum": true,
  "?minItems": "$posInt",
  "?maxItems": "$posInt",
  "?pattern": "$REGEX",
  "?minLength": "$posInt",
  "?maxLength": "$posInt",
  "?enum": {"@": ["$ANY"], ">=": 1, "!": true},
  "?uniqueItems": true,
  "?title": "",
  "?description": "",
  "?format": {"|": ["date-time", "date", "time", "utc-millisec", "regex", "color", "style", "phone", "uri", "email", "ip-address", "ipv6", "host-name"]},
  "?default": "$ANY",
  "?divisibleBy": "$strictPosNum",
  "?disallow": {"|": ["", "$distinctSchemaArray"]},
  "?extends": {"|": ["$Schema", ["$Schema"]]},
  "?id": "",
  "?$schema": "",
  "?$ref": "$URI"
}
