{
  "#": "JSON Model for JSON Schema Draft 04",
  "$": "Schema",
  "%": {
    "schemaArray": {"@": ["$Schema"], "ge": 1},
    "posInt": {"@": 0, "ge": 0},
    "simpleTypes": {"|": ["null", "boolean", "integer", "number", "string", "array", "object"]},
    "stringArray": {"@": [""], "ge": 1, "distinct": true},
    "typeArray": {"@": ["$simpleTypes"], "ge": 1, "distinct": true}
  },
  "?id": "",
  "?$schema": "",
  "?title": "",
  "?description": "",
  "?default": "$ANY",
  "?multipleOf": {"@": 0.0, "gt": 0.0},
  "?maximum": 0.0,
  "?exclusiveMaximum": true,
  "?minimum": 0.0,
  "?exclusiveMinimum": true,
  "?maxLength": "$posInt",
  "?minLength": "$posInt",
  "?pattern": "$REGEX",
  "?additionalItems": {"|": [true, "$Schema"]},
  "?items": {"|": ["$Schema", "$schemaArray"]},
  "?maxItems": "$posInt",
  "?minItems": "$posInt",
  "?uniqueItems": true,
  "?maxProperties": "$posInt",
  "?minProperties": "$posInt",
  "?uniqueItems": true,
  "?required": "$stringArray",
  "?additionalProperties": {"|": [true, "$Schema"]},
  "?definitions": {"": "$Schema"},
  "?properties": {"": "$Schema"},
  "?patternProperties": {"": "$Schema"},
  "?dependencies": {"": {"|": ["$Schema", "$stringArray"]}},
  "?enum": {"@": ["$ANY"], "ge": 1, "distinct": true},
  "?type": {"|": ["$simpleTypes", "$typeArray"]},
  "?format": "",
  "?allOf": "$schemaArray",
  "?anyOf": "$schemaArray",
  "?oneOf": "$schemaArray",
  "?not": "$Schema",
  "?$ref": "$URI"
}
