{
  "#": "JSON Model for JSON Schema Draft 07 [JSON_MODEL_LOOSE_INT, JSON_MODEL_LOOSE_FLOAT]",
  "~": "https://json-model.org/models/json-model",
  "$": {
    "": "https://json-model.org/models/json-schema-draft-07",
    "URI-REFERENCE": "",
    "schemaArray": {"@": ["$Schema"], ">=": 1},
    "simpleTypes": {"|": ["null", "boolean", "integer", "number", "string", "array", "object"]},
    "arrayTypes": {"@": ["$simpleTypes"], ">=": 1, "!": true},
    "ObjectSchema": {
	  "?$id": "$URI-REFERENCE",
	  "?$schema": "$URI",
      "?$comment": "",
	  "?title": "",
	  "?description": "",
	  "?default": "$ANY",
      "?readOnly": true,
      "?writeOnly": true,
      "?examples": ["$ANY"],
	  "?multipleOf": 1.0,
	  "?maximum": -1.0,
	  "?exclusiveMaximum": -1.0,
	  "?minimum": -1.0,
	  "?exclusiveMinimum": -1.0,
	  "?maxLength": 0,
	  "?minLength": 0,
	  "?pattern": "$REGEX",
	  "?additionalItems": "$Schema",
	  "?items": {"|": ["$Schema", "$schemaArray"]},
	  "?maxItems": 0,
	  "?minItems": 0,
	  "?uniqueItems": true,
      "?contains": "$Schema",
	  "?maxProperties": 0,
	  "?minProperties": 0,
	  "?required": [""],
	  "?additionalProperties": "$Schema",
	  "?definitions": {"": "$Schema"},
	  "?properties": {"": "$Schema"},
      "?propertyNames": "$Schema",
	  "?patternProperties": {"$REGEX": "$Schema"},
	  "?dependencies": {"": {"|": ["$Schema", [""]]}},
	  "?enum": {"@": ["$ANY"], ">=": 1, "!": true},
      "?const": "$ANY",
	  "?type": {"|": ["$simpleTypes", "$arrayTypes"]},
	  "?format": "",
      "?contentMediaType": "",
      "?contentEncoding": "",
	  "?if": "$Schema",
	  "?then": "$Schema",
	  "?else": "$Schema",
	  "?allOf": "$schemaArray",
	  "?anyOf": "$schemaArray",
	  "?oneOf": "$schemaArray",
	  "?not": "$Schema",
	  "?$ref": "$URI-REFERENCE"
    },
    "Schema": {"|": [true, "$ObjectSchema"]}
  },
  "@": "$Schema"
}
