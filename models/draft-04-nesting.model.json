{
  "#": "JSON Model for JSON Schema Draft 04",
  "$": "Schema",
  "%": {
    "schemaArray": {"@": ["$Schema"], ">=": 1},
    "simpleTypes": {"|": ["null", "boolean", "integer", "number", "string", "array", "object"]},
    "stringArray": {"@": [""], ">=": 1, "!": true},
    "typeArray": {"@": ["$simpleTypes"], ">=": 1, "!": true}
  },
  "?id": "",
  "?$schema": "",
  "?title": "",
  "?description": "",
  "?default": "$ANY",
  "?multipleOf": 1.0,
  "?maximum": 0.0,
  "?exclusiveMaximum": true,
  "?minimum": 0.0,
  "?exclusiveMinimum": true,
  "?maxLength": 0,
  "?minLength": 0,
  "?pattern": "$REGEX",
  "?additionalItems": {"|": [true, "$Schema"]},
  "?items": {"|": ["$Schema", "$schemaArray"]},
  "?maxItems": 0,
  "?minItems": 0,
  "?uniqueItems": true,
  "?maxProperties": 0,
  "?minProperties": 0,
  "?uniqueItems": true,
  "?required": "$stringArray",
  "?additionalProperties": {"|": [true, "$Schema"]},
  "?definitions": {"": "$Schema"},
  "?properties": {"": "$Schema"},
  "?patternProperties": {"": "$Schema"},
  "?dependencies": {"": {"|": ["$Schema", "$stringArray"]}},
  "?enum": {"@": ["$ANY"], ">=": 1, "!": true},
  "?type": {"|": ["$simpleTypes", "$typeArray"]},
  "?format": "",
  "?allOf": "$schemaArray",
  "?anyOf": "$schemaArray",
  "?oneOf": "$schemaArray",
  "?not": "$Schema",
  "?$ref": "$URI",
  "": {
    "#": "Schema nesting results in anything to be accepted",
    "@": "$Schema"
  }
}
