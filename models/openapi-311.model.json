{
  "#": "OpenAPI v3.1.1",
  "#.doc": "version 2024-10-24, see [spec](https://spec.openapis.org/oas/v3.1.1)",
  "~": "https://json-model.org/models/json-model",
  "$": {
    "": "https://json-model.org/models/openapi-311",
    "OpenAPI": {
      "openapi": "/^3\\.1\\.\\d+(-.+)?$/",
      "info": "$Info",
      "?jsonSchemaDialect": "$URL",
      "?servers": [ "$Server" ],
      "?paths": "$Paths",
      "?webhooks": { "": "$PathItem" },
      "?components": "$Components",
      "?security": [ "$SecurityRequirement" ],
      "?tags": [ "$Tag" ],
      "?externalDocs": "$ExternalDocumentation",
      "/^x-/": "$ANY"
    },
    "Info": {
      "title": "",
      "?summary": "",
      "?description": "",
      "?termsOfService": "",
      "?contact": "$Contact",
      "?license": "$License",
      "version": "",
      "/^x-/": "$ANY"
    },
    "Contact": {
      "?name": "",
      "?url": "$URL",
      "?email": "",
      "/^x-/": "$ANY"
    },
    "License": {
      "|": [
        { "name": "", "identifier": "", "/^x\\-.*$/": "$ANY" },
        { "name": "", "url": "$URL", "/^x\\-.*$/": "$ANY" }
      ]
    },
    "Server": {
      "url": "$URL",
      "?description": "",
      "?variables": { "": "$ServerVariable" },
      "/^x-/": "$ANY"
    },
    "ServerVariable": {
      "?enum": [""],
      "default": "",
      "?description": "",
      "/^x-/": "$ANY"
    },
    "Components": {
      "?schemas": { "": "$Schema" },
      "?responses": { "": { "|": [ "$Response", "$Reference" ] }},
      "?parameters": { "": { "|": [ "$Parameter", "$Reference" ] }},
      "?examples": { "": { "|": [ "$Example", "$Reference" ] }},
      "?requestBodies": { "": { "|": [ "$RequestBody", "$Reference" ] }},
      "?headers": { "": { "|": [ "$Header", "$Reference" ] }},
      "?securitySchemes": { "": { "|": [ "$SecurityScheme", "$Reference" ] }},
      "?links": { "": { "|": [ "$Link", "$Reference" ] }},
      "?callbacks": { "": { "|": [ "$Callback", "$Reference" ] }},
      "?pathItems": { "": { "|": [ "$PathItem", "$Reference" ] }},
      "/^x-/": "$ANY"
    },
    "Paths": {
      "/^//": "$PathItem",
      "/^x-/": "$ANY"
    },
    "PathItem": {
      "?$ref": "$URI",
      "?summary": "",
      "?description": "",
      "?get": "$Operation",
      "?put": "$Operation",
      "?post": "$Operation",
      "?delete": "$Operation",
      "?options": "$Operation",
      "?head": "$Operation",
      "?patch": "$Operation",
      "?trace": "$Operation",
      "?servers": [ "$Operation" ],
      "?parameters": [ { "|": [ "$Parameter", "$Reference" ] } ],
      "/^x-/": "$ANY"
    },
    "Operation": {
      "?tags": [ "" ],
      "?summary": "",
      "?description": "",
      "?externalDocs": "$ExternalDocumentation",
      "?operationId": "",
      "?parameters": [ { "|": [ "$Parameter", "$Reference" ] } ],
      "?requestBody": { "|": [ "$RequestBody", "$Reference" ] },
      "?responses": "$Responses",
      "?callbacks": { "": { "|": [ "$Callback", "$Reference" ] } },
      "?deprecated": true,
      "?security": [ "$SecurityRequirement" ],
      "?servers": [ "$Server" ],
      "/^x-/": "$ANY"
    },
    "ExternalDocumentation": {
      "?description": "",
      "url": "$URL",
      "/^x-/": "$ANY"
    },
    "parameterShare": {
      "?description": "",
      "?required": true,
      "?deprecated": true,
      "?allowEmptyValue": true,
      "/^x-/": "$ANY"
    },
    "commonParameter": {
      "+": [
        {
          "name": "",
          "in": { "|": [ "query", "header", "path", "cookie" ] }
        },
        "$parameterShare"
      ]
    },
    "Style": {
      "#": "serialization styles",
      "|": [ "matrix", "label", "form", "simple", "spaceDelimited", "pipeDelimited", "deepObject" ]
    },
    "parameterSchemaOnly": {
      "?style": "$Style",
      "?explode": true,
      "?allowReserved": true,
      "?schema": "$Schema",
      "?example": "$ANY",
      "?examples": { "": { "|": [ "$Example", "$Reference" ] }}
    },
    "parameterContentOnly": {
      "?content": { "": "$MediaType" }
    },
    "Parameter": {
      "+": [
        "$commonParameter",
        { "|": [ "$parameterSchemaOnly", "$parameterContentOnly" ] }
      ]
    },
    "RequestBody": {
      "?description": "",
      "?content": { "": "$MediaType" },
      "?required": true,
      "/^x-/": "$ANY"
    },
    "MediaType": {
      "?schema": "$Schema",
      "?example": "$ANY",
      "?examples": { "": { "|": [ "$Example", "$Reference" ] }},
      "?encoding": { "": "$Encoding" },
      "/^x-/": "$ANY"
    },
    "Encoding": {
      "?contentType": "",
      "?headers": { "": { "|": [ "$Header", "$Reference" ] }},
      "?style": "$Style",
      "?explode": true,
      "?allowReserved": true,
      "/^x-/": "$ANY"
    },
    "Responses": {
      "/^[1-5](\\d\\d|XX)$/": { "|": [ "$Response", "$Reference" ] },
      "?default": { "|": [ "$Response", "$Reference" ] },
      "/^x-/": "$ANY"
    },
    "Response": {
      "?description": "",
      "?headers": { "": { "|": [ "$Header", "$Reference" ] } },
      "?content": { "": "$MediaType" },
      "?links": { "": { "|": [ "$Link", "$Reference" ] } },
      "/^x-/": "$ANY"
    },
    "Callback": {
      "$Expression": { "|": [ "$PathItem", "$Reference" ] },
      "/^x-/": "$ANY"
    },
    "Example": {
      "?summary": "",
      "?description": "",
      "?value": "$ANY",
      "?externalValue": "",
      "/^x-/": "$ANY"
    },
    "Expression": "",
    "Link": {
      "+": [
        { "|": [ { "operationRef": "" }, { "operationId": "" } ] },
        {
          "?parameters": { "": { "|": ["$Expression", "$ANY"] } },
          "?requestBody": { "|": ["$Expression", "$ANY"] },
          "?description": "",
          "?server": "$Server",
          "/^x-/": "$ANY"
        }
      ]
    },
    "Header": {
      "#": "more or less a parameter but without name and in",
      "+": [
        "$parameterShare",
        { "|": [ "$parameterSchemaOnly", "$parameterContentOnly" ] }
      ]
    },
    "Tag": {
      "name": "",
      "?description": "",
      "?externalDocs": "$ExternalDocumentation",
      "/^x-/": "$ANY"
    },
    "Reference": {
      "_$ref": "",
      "?summary": "",
      "?description": ""
    },
    "schema": "$https://json-model.org/models/json-schema-draft-2020-12",
    "Schema": {
      "#": "OpenAPI Schema is only described with an object and adds a few keywords",
      "+": [
        "$schema#ObjectSchema",
        {
          "?discriminator": "$Discriminator",
          "?externalDocs": "$ExternalDocumentation",
          "?xml": "$xml",
          "?example": "$ANY"
        }
      ]
    },
    "Discriminator": {
      "propertyName": "",
      "?mapping": { "": "" },
      "/^x-/": "$ANY"
    },
    "xml": {
      "?name": "",
      "?namespace": "",
      "?prefix": "",
      "?attribute": true,
      "?wrapped": true,
      "/^x-/": "$ANY"
    },
    "SS-apikey": {
      "type": "_apiKey",
      "name": "",
      "in": { "|": ["query", "header", "cookie"] }
    },
    "SS-http": {
      "type": "_http",
      "scheme": ""
    },
    "SS-http-bearer": {
      "type": "_http",
      "scheme": "/bearer/i",
      "?bearerFormat": ""
    },
    "SS-oauth2": {
      "type": "_oauth2",
      "flows": "$OAuthFlows"
    },
    "SS-oic": {
      "type": "_openIdConnect",
      "openIdConnectUrl": "$URL"
    },
    "SecurityScheme": {
      "+": [
        { "|": [ "$SS-apikey", "$SS-http-bearer", "$SS-http", "$SS-oauth2", "$SS-oic", "$Extension" ] },
        { "?description": "" }
      ]
    },
    "OAuthFlows": {
      "?implicit": "$OAuthFlow",
      "?password": "$OAuthFlow",
      "?clientCredentials": "$OAuthFlow",
      "?authorizationCode": "$OAuthFlow",
      "/^x-/": "$ANY"
    },
    "OAuthFlow": {
      "authorizationUrl": "$URL",
      "tokenUrl": "$URL",
      "?refreshUrl": "$URL",
      "scopes": { "": "" },
      "/^x-/": "$ANY"
    },
    "SecurityRequirement": { "": [ "" ] },
    "Extension": { "/^x-/": "$ANY" }
  },
  "@": "$OpenAPI"
}
