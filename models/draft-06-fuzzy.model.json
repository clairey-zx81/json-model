{
  "#": {
    "#": "Fuzzy JSON Model for JSON Schema Draft 06",
    "options": ["loose-int"]
  },
  "$": "Schema",
  "%": {
    "schemaArray": {"@": ["$Schema"], "ge": 1},
    "posInt": {"@": 0, "ge": 0},
    "simpleTypes": {"|": ["null", "boolean", "integer", "number", "string", "array", "object"]},
    "typeArray": {"@": ["$simpleTypes"], "ge": 1},
    "stringArray": [""],
    "ObjectSchema": {
	  "?$id": "$URI-REFERENCE",
	  "?$schema": "$URI",
	  "?title": "",
	  "?description": "",
	  "?default": "$ANY",
      "?examples": ["$ANY"],
	  "?multipleOf": {"@": 0.0, "gt": 0.0},
	  "?maximum": 0.0,
	  "?exclusiveMaximum": 0.0,
	  "?minimum": 0.0,
	  "?exclusiveMinimum": 0.0,
	  "?maxLength": "$posInt",
	  "?minLength": "$posInt",
	  "?pattern": "",
	  "?additionalItems": "$Schema",
	  "?items": {"|": ["$Schema", "$schemaArray"]},
	  "?maxItems": "$posInt",
	  "?minItems": "$posInt",
	  "?uniqueItems": true,
      "?contains": "$Schema",
	  "?maxProperties": "$posInt",
	  "?minProperties": "$posInt",
	  "?uniqueItems": true,
	  "?required": "$stringArray",
	  "?additionalProperties": "$Schema",
	  "?definitions": {"": "$Schema"},
	  "?properties": {"": "$Schema"},
      "?propertyNames": "$Schema",
	  "?patternProperties": {"": "$Schema"},
	  "?dependencies": {"": {"|": ["$Schema", "$stringArray"]}},
	  "?enum": {"@": ["$ANY"], "ge": 1, "distinct": true},
      "?const": "$ANY",
	  "?type": {"|": ["$simpleTypes", "$typeArray"]},
	  "?format": "",
	  "?allOf": "$schemaArray",
	  "?anyOf": "$schemaArray",
	  "?oneOf": "$schemaArray",
	  "?not": "$Schema",
	  "?$ref": "$URI-REFERENCE",
      "": "$ANY"
    }
  },
  "|": [true, "$ObjectSchema"]
}
