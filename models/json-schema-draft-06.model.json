{
  "#": "JSON Model for JSON Schema Draft 06 [JSON_MODEL_LOOSE_INT, JSON_MODEL_LOOSE_FLOAT]",
  "~": "https://json-model.org/models/json-model",
  "$": {
    "": "https://json-model.org/models/json-schema-draft-06",
    "URI-REFERENCE": "",
    "schemaArray": {"@": ["$Schema"], ">=": 1},
    "simpleTypes": {"|": ["null", "boolean", "integer", "number", "string", "array", "object"]},
    "typeArray": {"@": ["$simpleTypes"], ">=": 1},
    "stringArray": [""],
    "ObjectSchema": {
      "?$id": "$URI-REFERENCE",
      "?$schema": "$URI",
      "?title": "",
      "?description": "",
      "?default": "$ANY",
      "?examples": ["$ANY"],
      "?multipleOf": 1.0,
      "?maximum": -1.0,
      "?exclusiveMaximum": -1.0,
      "?minimum": -1.0,
      "?exclusiveMinimum": -1.0,
      "?maxLength": 0,
      "?minLength": 0,
      "?pattern": "$REGEX",
      "?additionalItems": "$Schema",
      "?items": {"|": ["$Schema", "$schemaArray"]},
      "?maxItems": 0,
      "?minItems": 0,
      "?uniqueItems": true,
      "?contains": "$Schema",
      "?maxProperties": 0,
      "?minProperties": 0,
      "?required": "$stringArray",
      "?additionalProperties": "$Schema",
      "?definitions": {"": "$Schema"},
      "?properties": {"": "$Schema"},
      "?propertyNames": "$Schema",
      "?patternProperties": {"$REGEX": "$Schema"},
      "?dependencies": {"": {"|": ["$Schema", "$stringArray"]}},
      "?enum": {"@": ["$ANY"], ">=": 1, "!": true},
      "?const": "$ANY",
      "?type": {"|": ["$simpleTypes", "$typeArray"]},
      "?format": "",
      "?allOf": "$schemaArray",
      "?anyOf": "$schemaArray",
      "?oneOf": "$schemaArray",
      "?not": "$Schema",
      "?$ref": "$URI-REFERENCE"
    },
    "Schema": {"|": [true, "$ObjectSchema"]}
  },
  "@": "$Schema"
}
