    "platforms": [ { "+": [ "$any_platform", {"": "$ANY"} ] } ],
    "GalaxyInfoModelCollection": {
      "?cloud_platforms": "$ANY",
      "?galaxy_tags": [ "" ],
      "?min_ansible_version": "",
      "?namespace": "/^[a-z][-a-z0-9_]+$/",
      "?platforms": "$platforms",
      "?role_name": "/^[a-z][a-z0-9_]+$/",
      "?video_links": [ "$ANY" ]
    },
    "GalaxyInfoModelLoose": {
      "#": "some $URL should be added?",
      "description": "",
      "?author": "",
      "?company": "",
      "?github_branch": "",
      "?issue_tracker_url": "",
      "?license": "",
      "?min_ansible_container_version": ""
    },
    "GalaxyInfoModelStandalone": {
      "#": "standalone info has 3 mandatory props",
      "+": [
        { "standalone": "=true" },
        "$GalaxyInfoModelLoose",
        "$GalaxyInfoModelCollection",
        { "author": "$ANY", "license": "$ANY", "min_ansible_version": "$ANY" }
      ]
    },
    "GalaxyInfoModel": {
      "#": "hmmmm, property standalone should probably be requiredâ€¦",
      "|": [
        "$GalaxyInfoModelStandalone",
        { "+": [ { "standalone": "=false" }, "$GalaxyInfoModelLoose", "$GalaxyInfoModelCollection" ] },
        { "+": [ "$GalaxyInfoModelLoose", "$GalaxyInfoModelCollection" ] }
      ]
    },
    "collections": [ "/^[a-z_]+\\.[a-z_]+$/" ],
    "complex_conditional": { "^": [ true, "", [""] ] },
    "DependencyModelLoose": {
      "#": "See https://docs.ansible.com/ansible/latest/playbook_guide/playbooks_reuse_roles.html#role-dependencies and https://github.com/ansible/ansible/blob/devel/lib/ansible/playbook/role/metadata.py#L79\n\nOther keys are treated as role [parameters](https://docs.ansible.com/ansible/latest/user_guide/playbooks_reuse_roles.html#passing-different-parameters).",
      "?become": true,
      "?name": "",
      "?role": "",
      "?src": "",
      "?scm": {"|": ["hg", "git"]},
      "?tags": {"|": ["", [""]]},
      "?vars": {"":"$ANY"},
      "?version": "",
      "?when": "$complex_conditional",
      "": "$ANY"
    },
    "DependencyModel": {
      "#": "add non-exclusive mandatory role, src or name props",
      "&": [
        "$DependencyModelLoose",
        { "|": [{"role": "", "": "$ANY"}, {"src": "", "": "$ANY"}, {"name": "", "": "$ANY"}] }
      ]
    },
    "AnsibleMetaObj": {
      "?allow_duplicates": true,
      "?collections": "$collections",
      "?dependencies": [ { "|": [ "", "$DependencyModel" ] } ],
      "?galaxy_info": "$GalaxyInfoModel"
    },
    "AnsibleMeta": {
      "#": "why null?",
      "|": [ null, "$AnsibleMetaObj" ]
    }
  },
  "@": "$AnsibleMeta"
