{
  "#": "Fuzzy JSON Model for JSON Schema Draft 03",
  "$": "Schema",
  "%": {
    "allTypes": {"|": ["null", "boolean", "integer", "number", "string", "array", "object", "any"]},
    "distinctSchemaArray": {"@": [{"|": ["", "$Schema"]}], "!": true}
  },
  "?type": {"|": ["$allTypes", [{"|": ["", "$Schema"]}]]},
  "?properties": { "": "$Schema" },
  "?patternProperties": {"": "$Schema"},
  "?additionalProperties": {"|": [true, "$Schema"]},
  "?items": {"|": ["$Schema", ["$Schema"]]},
  "?additionalItems": {"|": [true, "$Schema"]},
  "?dependencies": {"": {"|": ["", [""], "$Schema"]}},
  "?required": true,
  "?minimum": -1.0,
  "?maximum": -1.0,
  "?exclusiveMaximum": true,
  "?exclusiveMinimum": true,
  "?minItems": 0,
  "?maxItems": 0,
  "?pattern": "",
  "?minLength": 0,
  "?maxLength": 0,
  "?enum": {"@": ["$ANY"], ">=": 1, "!": true},
  "?uniqueItems": true,
  "?title": "",
  "?description": "",
  "?format": "",
  "?default": "$ANY",
  "?divisibleBy": 1.0,
  "?disallow": {"|": ["", "$distinctSchemaArray"]},
  "?extends": {"|": ["$Schema", ["$Schema"]]},
  "?id": "",
  "?$schema": "",
  "?$ref": "$URI",
  "?definitions": { "": "$Schema" },
  "?links": [ {"?rel": "", "?href": ""} ],
  "": "$ANY"
}
